
{
  "expo": {
    "name": "Intentional",
    "slug": "intentional",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/f03496a1-9a34-4437-93f3-131ede024e31.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/f03496a1-9a34-4437-93f3-131ede024e31.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a1a1a"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.intentional.dating",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/f03496a1-9a34-4437-93f3-131ede024e31.png",
        "backgroundColor": "#1a1a1a"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.intentional.dating"
    },
    "web": {
      "favicon": "./assets/images/f03496a1-9a34-4437-93f3-131ede024e31.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser"
    ],
    "scheme": "intentional",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "YOUR_PROJECT_ID"
      }
    }
  }
}
